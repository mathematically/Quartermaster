<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:Mathematically.Quartermaster.Converters">
    <converters:EnumDescriptionConverter x:Key="EnumDescriptionConverter" />
    <converters:NullVisibilityConverter x:Key="CollapsedIfNull" IsNotNull="Visible" IsNull="Collapsed" />
    <converters:ZeroDoubleVisibilityConverter x:Key="CollapsedIfZero" IsZero="Collapsed" IsNotZero="Visible" />
    <converters:OffsetTextConverter x:Key="OffsetTextConverter" />
    <converters:HideLargeOffsetConverter x:Key="HideLargeOffsetConverter" />
    <converters:LevelOffsetColorConverter x:Key="LevelOffsetColorConverter" />
</ResourceDictionary>